<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.weasel" name="WeAsEl Test Add-on" version="0.0.1" provider-name="WeAsEl">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Anime</summary>
		<description lang="en">Anime</description>
		<platform>android</platform>
		<forum>http://www.noobsandnerds.com/support</forum>
		<website>www.noobsandnerds.com</website>
		<news></news>
		<genre>anime</genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>

<addon id="repository.weasel" name="WeAsEl Repo" version="0.0.1" provider-name="WeAsEl">
	<extension point="xbmc.addon.repository" name="WeAsEl Repo">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/totalrevolution/python-koding/master/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/totalrevolution/python-koding/master/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/totalrevolution/python-koding/master/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/WeAsEl/repository.weasel/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/WeAsEl/repository.weasel/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/WeAsEl/repository.weasel/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">anime</summary>
		<description lang="en">Anime</description>
		<genre>anime</genre>
	</extension>
</addon>
</addons>
